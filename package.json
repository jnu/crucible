{
  "name": "crucible",
  "version": "1.0.0",
  "description": "Crossword constructor",
  "main": "index.js",
  "scripts": {
    "checknode": "([[ \"$(node -v)\" == \"v8.9\"* ]] || (echo \"\\033[97m\\033[41m>>> Crucible requires node v8.9. <<<\\033[0m\"; false))",
    "start": "npm run checknode && NODE_ENV=local $(npm bin)/webpack-dev-server --content-base build/ --port 3000 --inline --hot",
    "build": "npm run checknode && NODE_ENV=production $(npm bin)/webpack --bail",
    "lint": "npm run checknode && eslint ./src",
    "test": "npm run checknode && npm run lint && karma start karma-unit.conf.js",
    "test-watch": "npm run checknode && karma start karma-unit.conf.js --auto-watch",
    "benchmark": "npm run checknode && karma start karma-perf.conf.js",
    "benchmark-watch": "npm run checknode && karma start karma-perf.conf.js --auto-watch"
  },
  "keywords": [
    "crossword",
    "builder",
    "constructor",
    "words",
    "cruciverb",
    "puzzle"
  ],
  "author": "Joe Nudell <joenudell@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.28.10",
    "es6-promise": "^4.0.3",
    "eslint": "^3.7.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "webpack-contrib/html-webpack-plugin",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-benchmark": "^0.7.1",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "2.0.13",
    "lodash": "^4.16.3",
    "material-ui": "^0.16.7",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.1",
    "node-sass": "^4.5.0",
    "pure-uuid": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^2.0.1",
    "react-virtualized": "^8.11.4",
    "recompose": "^0.20.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "tiny-trie": "0.2.5",
    "ts-loader": "^4.0.1",
    "typescript": "^2.7.2",
    "utf8": "^2.1.1",
    "webpack": "4",
    "webpack-dev-middleware": "3",
    "webpack-dev-server": "^3.1.1"
  },
  "devDependencies": {
    "webpack-cli": "^2.0.12"
  }
}
